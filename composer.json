{
    "name": "wavey/sweetalert",
    "description": "SweetAlert2 implementation for Laravel",
    "keywords": [
        "sweetalert",
        "laravel",
        "sweet",
        "alert",
        "alerts"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Kieran Heron",
            "email": "kieran@wavey.group"
        }
    ],
    "support": {
        "issues": "https://github.com/waveygroup/sweetalert/issues",
        "source": "https://github.com/waveygroup/sweetalert"
    },
    "require": {
        "php": "^8.0|^8.3",
        "laravel": "^10.0|^11.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Wavey\\Sweetalert\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wavey\\Sweetalert\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Wavey\\Sweetalert\\SweetalertServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
